<div class="user-container">
    <div class="row top-30">
        <div class="col-md-2 col-sm-2">
            <a class="btn btn-default" id="viewDiaryButton" href="/diary">Go to Your Diary</a>
        </div>
        <div class="col-md-2 col-sm-2">
            <a class="btn btn-default" href="/logout">Logout</a>
        </div>
        </div>
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <form id="user">
                        <div class="form-group">
                            <label for="firstName">First Name:</label>
                            <input placeholder="First Name" type="text" class="form-control" id="firstName">
                            <br />
                            <label for="lastName">Last Name:</label>
                            <input placeholder="Last Name" type="text" class="form-control" id="lastName">
                            <br />
                            <label for="email">Email Address:</label>
                            <input placeholder="Email Address" type="text" class="form-control" id="email">
                            <br />
                            <label for="password1">Password:</label>
                            <input placeholder="Password" type="text" class="form-control" id="password1">
                            <br />
                            <label for="password2">Password Confirm:</label>
                            <input placeholder="Confirm Password" type="text" class="form-control" id="password2">
                            <br />
                            <label for="language">Language Preference:</label>
                            <input placeholder="Language Preference" type="text" class="form-control" id="language">
                            <br />
                            <label for="voice">Voice Preference:</label>
                            <input placeholder="Voice Preference" type="text" class="form-control" id="voice">
                            <br />
                            <label for="timeZone">Time Zone:</label>
                            <input placeholder="Time Zone" type="text" class="form-control" id="timeZone">
                            <br />
                            <button type="submit" class="btn btn-success submit">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Custom Script -->
<script src="../js/user.js" type="text/javascript"></script>